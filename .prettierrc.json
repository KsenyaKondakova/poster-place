{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "<BUILT_IN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/assets/(.*)$",
    "",
    "^@/components/(.*)$",
    "",
    "^@/redux/(.*)$",
    "",
    "^@/types/(.*)$",
    "",
    "^[.]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
